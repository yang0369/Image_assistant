{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: Run RAG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/project/src/vector_db/build_slr_vector_db.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/project",
                "PYDEVD_WARN_EVALUATION_TIMEOUT": "120",
            }
        }
    ]
}